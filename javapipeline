pipeline    {
            agent any
            stages {
             stage('BUILD'){
                steps {
                        git branch: 'main', url :'https://github.com/reddyprashanth305/java-.xmltest-.git'
                        sh 'mvn clean install'
                    }
                  }
                  stage('DEPLOY') {
                   steps {
                        sh 'echo "javapipeline"'
             	 }
				     }
			        	stage('STATUS') {
					      steps {
						      sh 'echo "pipeline job succefully build in slave-01 and deployed in slave-02 in tomcat webapp folder"'	
					}
				}
			}
		}
